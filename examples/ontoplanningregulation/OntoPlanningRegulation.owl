    <!-- https://graphdb.accordproject.eu/kg/ontoplanningregulation/AbsoluteHeight -->


    <owl:Class rdf:about="https://graphdb.accordproject.eu/kg/ontoplanningregulation/AbsoluteHeight">
        <rdfs:subClassOf rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Height"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Absolute Height defines the total vertical measurement of the building from its lowest underground storey to the highest roof. It should account for all storeys and their heights in this calculation</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://graphdb.accordproject.eu/kg/ontoplanningregulation/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Absolute Height</rdfs:label>
    </owl:Class>
    


