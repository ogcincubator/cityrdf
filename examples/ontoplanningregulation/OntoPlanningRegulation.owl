    <!-- https://graphdb.accordproject.eu/kg/ontoplanningregulation/appliesTo -->


    <owl:ObjectProperty rdf:about="https://graphdb.accordproject.eu/kg/ontoplanningregulation/appliesTo">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://graphdb.accordproject.eu/kg/ontoplanningregulation/LandUseRegulation"/>
                    <rdf:Description rdf:about="https://graphdb.accordproject.eu/kg/ontoplanningregulation/PlanningRegulation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://graphdb.accordproject.eu/kg/ontoplot/Plot"/>
                    <rdf:Description rdf:about="https://graphdb.accordproject.eu/kg/ontozoning/LandUseType"/>
                    <rdf:Description rdf:about="https://graphdb.accordproject.eu/kg/ontozoning/Zone"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This relationship serves as a link to signify the direct relevance and applicability of the planning regulation to that specific urban entity.  In the case of land use regulations, they are applicable only for land use types.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://graphdb.accordproject.eu/kg/ontoplanningregulation/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">applies to</rdfs:label>
    </owl:ObjectProperty>
    


