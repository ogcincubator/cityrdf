@prefix app: <https://ogcblocks.org/CityGML/3.0/appearance#> .
@prefix bldg: <https://ogcblocks.org/CityGML/3.0/building#> .
@prefix brid: <https://ogcblocks.org/CityGML/3.0/bridge#> .
@prefix con: <https://ogcblocks.org/CityGML/3.0/construction#> .
@prefix core: <https://ogcblocks.org/CityGML/3.0/core#> .
@prefix dem: <https://ogcblocks.org/CityGML/3.0/relief#> .
@prefix dyn: <https://ogcblocks.org/CityGML/3.0/dynamizer#> .
@prefix frn: <https://ogcblocks.org/CityGML/3.0/cityfurniture#> .
@prefix gen: <https://ogcblocks.org/CityGML/3.0/generics#> .
@prefix gmlowl: <http://www.opengis.net/ont/gml#> .
@prefix grp: <https://ogcblocks.org/CityGML/3.0/cityobjectgroup#> .
