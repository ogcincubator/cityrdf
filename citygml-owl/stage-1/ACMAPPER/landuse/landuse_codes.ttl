@prefix dct:      <http://purl.org/dc/terms/> .
@prefix lusecode: <https://www.opengis.net/ont/citygml/landuse/code#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:     <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .

lusecode:LandUseUsageValue
        rdf:type         skos:ConceptScheme;
        rdfs:label       "LandUseUsageValue"@en;
        dct:isFormatOf   <https://www.opengis.net/ont/citygml/landuse/LandUseUsageValue>;
        skos:definition  "LandUseUsageValue is a code list that enumerates the different uses of a LandUse."@en .

<https://www.opengis.net/ont/citygml/landuse/code>
        rdf:type  owl:Ontology .

lusecode:LandUseClassValue
        rdf:type         skos:ConceptScheme;
        rdfs:label       "LandUseClassValue"@en;
        dct:isFormatOf   <https://www.opengis.net/ont/citygml/landuse/LandUseClassValue>;
        skos:definition  "LandUseClassValue is a code list used to further classify a LandUse."@en .

lusecode:LandUseFunctionValue
        rdf:type         skos:ConceptScheme;
        rdfs:label       "LandUseFunctionValue"@en;
        dct:isFormatOf   <https://www.opengis.net/ont/citygml/landuse/LandUseFunctionValue>;
        skos:definition  "LandUseFunctionValue is a code list that enumerates the different purposes of a LandUse."@en .
