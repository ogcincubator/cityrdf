  <rdf:Description rdf:about="https://www.opengis.net/ont/citygml/cityfurniture">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <rdfs:label xml:lang="en">CityFurniture</rdfs:label>
    <owl:imports rdf:resource="http://def.isotc211.org/iso19150/-2/2012/base"/>
    <owl:imports rdf:resource="https://www.opengis.net/ont/citygml/common"/>
    <owl:imports rdf:resource="https://www.opengis.net/ont/citygml/core"/>
    <owl:versionIRI rdf:resource="https://www.opengis.net/ont/citygml/cityfurniture/3.0.0/"/>
    <owl:versionInfo>3.0.0</owl:versionInfo>
    <skos:definition xml:lang="en">The CityFurniture module supports representation of city furniture objects. City furniture objects are immovable objects like lanterns, traffic signs, advertising columns, benches, or bus stops that can be found in traffic areas, residential areas, on squares, or in built-up areas.</skos:definition>
  </rdf:Description>
